
@CompileDef
@controller.RESTController("s")
primitive rest.Server {
  provides rest.Service as s;
  
  source {{
    #include <stdio.h>
    
    int METH(s, add)(int a, int b) {
      printf("Server.s.add(a=%d, b=%d)\n", a, b);
      return a+b;
    }
    void METH(s, print)(string s) {
      printf("Server.s.print: s=%s\n", s);
    }
  }}
}