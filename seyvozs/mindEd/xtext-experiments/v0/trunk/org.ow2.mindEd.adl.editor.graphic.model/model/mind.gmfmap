      <audits
          name="Component name check"
          description="This will check if a component's name is correct"
          id="component_namecheck"
          message="Component name not valid">
        <rule
            body="self.name.size()>0"/>
        <target
            xsi:type="gmfmap:DomainElementTarget">
          <element
              href="../../org.ow2.mindEd.adl.schema/model/mind.ecore#//ArchitectureDefinition"/>
        </target>
      </audits>
      <audits
          name="interface_name_check"
          description="This will check that all interfaces have a name"
          id="interface_name_check"
          message="Interface must have a name">
        <rule
            body="self.name.size() > 0"/>
        <target
            xsi:type="gmfmap:DomainElementTarget">
          <element
              href="../../org.ow2.mindEd.adl.schema/model/mind.ecore#//InterfaceDefinition"/>
        </target>
      </audits>
      <audits
