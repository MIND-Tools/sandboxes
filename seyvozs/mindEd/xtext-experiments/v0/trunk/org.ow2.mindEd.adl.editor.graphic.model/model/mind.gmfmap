          description="Duplicate name for subcomponents"
          id="subcomp_same_name"
          useInLiveMode="true"
          message="Duplicate name">
        <rule
            body="return org.ow2.mindEd.adl.editor.graphic.ui.custom.constraints.JavaConstraints.subCompSameName(context, ctx);"
            language="java"/>
        <target
            xsi:type="gmfmap:DomainElementTarget">
          <element
              href="../../org.ow2.mindEd.adl.schema/model/mind.ecore#//SubComponentDefinition"/>
        </target>
      </audits>
    </childContainers>
    <childContainers
        id="Code"
        name="Code"
        description="Code">
      <audits
          name="inline_pattern"
          description="This will check that an inline c code begins and ends with double curly brackets"
          id="inline_pattern"
          message="Inline code must begin and end with double curly brackets">
        <rule
            body="let s : String = self.codeC in&#xD;&#xA;( s.size() > 3 )&#xD;&#xA;and&#xD;&#xA;( s.substring(1,2) = '{{' )&#xD;&#xA;and&#xD;&#xA;( s.substring(s.size()-1,s.size()) = '}}' )"/>
        <target
            xsi:type="gmfmap:DomainElementTarget">
          <element
              href="../../org.ow2.mindEd.adl.schema/model/mind.ecore#//InlineCodeC"/>
        </target>
      </audits>
      <audits
          name="data_content"
          description="This will check that datas only has one content"
          id="data_content"
          useInLiveMode="true"
          message="Data must be File or Inline">
        <rule
            body="(self.fileC=null or self.inlineCcode=null)"/>
        <target
            xsi:type="gmfmap:DomainElementTarget">
          <element
              href="../../org.ow2.mindEd.adl.schema/model/mind.ecore#//DataDefinition"/>
        </target>
      </audits>
      <audits
          name="source_not_empty"
          description="This will check that an implementation is not empty"
          id="source_not_empty"
          message="Source must not be empty">
        <rule
            body="not(self.fileC=null and self.inlineCcode=null)"/>
        <target
            xsi:type="gmfmap:DomainElementTarget">
          <element
              href="../../org.ow2.mindEd.adl.schema/model/mind.ecore#//ImplementationDefinition"/>
        </target>
      </audits>
      <audits
          name="source_content"
          description="This will check that sources only has one content"
          id="source_content"
          useInLiveMode="true"
          message="Source must be File or Inline">
        <rule
            body="(self.fileC=null or self.inlineCcode=null)"/>
        <target
            xsi:type="gmfmap:DomainElementTarget">
          <element
              href="../../org.ow2.mindEd.adl.schema/model/mind.ecore#//ImplementationDefinition"/>
        </target>
      </audits>
    </childContainers>
